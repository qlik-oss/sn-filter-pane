{
  "name": "sn-filter-pane",
  "version": "0.1.0",
  "description": "Just a Filter pane.",
  "license": "MIT",
  "author": {
    "name": "Johan Lahti",
    "email": "johan.lahti@qlik.com"
  },
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-filter-pane.js",
  "module": "dist/sn-filter-pane.esm.js",
  "scripts": {
    "start": "NODE_ENV=development nebula serve --type filterpane --preserve-symlinks",
    "build": "NODE_ENV=production nebula build --ext --mode production",
    "build:dev": "NODE_ENV=development nebula build --ext --mode development --preserve-symlinks",
    "build:watch": "nebula build --ext --mode development -w --preserve-symlinks",
    "lint": "eslint src",
    "sense": "nebula sense --ext src/ext.js --meta src/meta.json",
    "test:integration": "aw puppet --testExt '*.int.js' --glob 'test/integration/**/*.int.js'"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/enigma.js": "^2.2.2",
    "@types/qlik-engineapi": "^12.67.12",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/systemjs": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "babel-eslint": "^10.1.0",
    "eslint": "8.23.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "zustand": "^4.1.1"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "*"
  },
  "dependencies": {
    "extend": "^3.0.2"
  }
}
